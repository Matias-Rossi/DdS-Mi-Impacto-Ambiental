{{#partial "contenido" }}

      <div class="d-flex flex-column" id="content-wrap">
          <div class=" d-flex align-self-center conteinerRegistro">
              <main>
                  <div class="row" style="margin:1vmax 1vmax 1vmax 1vmax;">
                      <div class="col-md-12 d-flex justify-content-center">
                          <label class="form-label textoInputs" style="font-size: 24px; ">Tipo de registro</label>
                      </div>
                  </div>



                  <div class="row" style="margin:1vmax 1vmax 1vmax 1vmax;">
                      <div class="col-md-12">
                          <div class="d-flex justify-content-center">
                              <button class=" btn-custom " data-bs-toggle="modal"
                                  data-bs-target="#registroUsuario ">Miembro</button>
                          </div>
                      </div>
                  </div>
                  <div class="row" style="margin:1vmax 1vmax 1vmax 1vmax;">
                      <div class="col-md-12">
                          <div class="d-flex justify-content-center">
                              <button class="btn-custom" style="background-color: #692C6D !important;"
                                  data-bs-toggle="modal" data-bs-target="#registroOrganizacion">Organizacion</button>
                          </div>
                      </div>
                  </div>
                  <div class="row" style="margin:1vmax 1vmax 1vmax 1vmax;">
                      <div class="col-md-12">
                          <div class="d-flex justify-content-center">
                              <button class=" btn-custom " style="background-color:#E44C25 !important;"
                                  data-bs-toggle="modal" data-bs-target="#registroAS">Agente
                                  sectorial</button>
                          </div>
                      </div>
                  </div>
              </main>
          </div>
          <div class="d-flex row justify-content-center" id="poseeCuenta">
              <span class="textoNoLink text-small col-sm-1" style="width: fit-content;" id="poseeCuenta1">¿Ya posee una
                  cuenta?&nbsp</span> <a href="Login.html" id="poseeCuenta2" class=" col-sm-1 textoLink"
                  style="width: fit-content;">Inicie Sesion</a>
          </div>
      </div>

      <!-- Modal Miembro -->
      <div class="modal fade" id="registroUsuario" tabindex="-1">
          <div class="modal-dialog modal-lg">
              <div class="modal-content">
                  <div class="modal-body">
                      <main>
                          <form class="was-validated" method="POST" action="/signup" novalidate>
                              <input name="tipoUsuario" style="color: rgba(0, 0, 0, 0);" value="MIEMBRO" type="hidden">
                              <div class="row" style="margin:1vmax 1vmax 1vmax 1vmax;">
                                  <div class="col-md-3">
                                      <label for="firstName" class="form-label textoInputs">Nombre</label>
                                      <input name="nombre" type="text" class="form-control" id="firstName" placeholder=""
                                          value="" required>
                                  </div>
                                  <div class="col-md-3">
                                      <label for="lastName" class="form-label textoInputs">Apellido</label>
                                      <input name="apellido" type="text" class="form-control" id="lastName" placeholder=""
                                          value="" required>
                                  </div>
                                  <div class="col-md-6">
                                      <label for="selectTipoDoc" class="form-label textoInputs">Tipo de documento</label>
                                      <select name="tipoDoc" class="form-select form-select-custom" id="selectTipoDoc"
                                          required>
                                          <option selected disabled value="">Elegir...</option>
                                          <option>DNI</option>
                                          <option>CUIL</option>
                                          <option>Cedula</option>
                                          <option>Libreta Civica</option>
                                          <option>Libreta de Enrolamiento</option>
                                      </select>
                                  </div>
                              </div>
                              <div class="row" style="margin:1vmax 1vmax 1vmax 1vmax;">
                                  <div class="col-md-6">
                                      <label for="inputDate" class="form-label textoInputs">Fecha de nacimiento</label>
                                      <input type="date" class="form-control" id="inputDate" placeholder="" value=""
                                          onclick="maxDate();" required>
                                  </div>
                                  <div class="col-md-6">
                                      <label for="inpudDocumento" class="form-label textoInputs">Numero de
                                          documento</label>
                                      <input name="numeroDoc" type="number" class="form-control" id="inpudDocumento"
                                          placeholder="" value="" min="0" required>
                                  </div>
                              </div>
                              <div class="row" style="margin:1vmax 1vmax 1vmax 1vmax;">
                                  <div class="col-md-6">
                                      <label for="inputCalle" class="form-label textoInputs">Direccion</label>
                                      <input name="calle" type="text" class="form-control" id="inputCalle" placeholder=""
                                          value="" required>
                                  </div>
                                  <div class="col-md-3">
                                      <label for="inputNumeracion" class="form-label textoInputs">Numeracion</label>
                                      <input name="numeracion" type="number" class="form-control" id="inputNumeracion"
                                          placeholder="" value="" min="0" required>
                                  </div>
                                  <div class="col-md-3">
                                      <label for="inputCP" class="form-label textoInputs">Codigo postal</label>
                                      <input name="codPostal" type="number" class="form-control" id="inputCP"
                                          placeholder="" value="" min="0" required>
                                  </div>
                              </div>
                              <div class="row" style="margin:1vmax 1vmax 1vmax 1vmax;">
                                  <div class="col-md-6">
                                      <label for="selectMunicipio" class="form-label textoInputs">Municipio</label>
                                      <select name="municipio" class="form-select form-select-custom" id="selectMunicipio"
                                          required>
                                          <option selected disabled value="">Elegir...</option>
                                          {{#each municipiosODepartamentos}}
                                            <option value="{{id}}">{{municipioOLocalidad}}</option>
                                            {{/each}}
                                      </select>
                                  </div>

                                  <div class="col-md-6">
                                      <label for="inputLocalidad" class="form-label textoInputs">Localidad</label>
                                      <input name="localidad" type="text" class="form-control" id="inputLocalidad"
                                          placeholder="" value="" required>
                                  </div>
                              </div>
                              <div class="row" style="margin:1vmax 1vmax 1vmax 1vmax;">
                                  <div class="col-md-6">
                                      <label for="inputEmail" class="form-label textoInputs">Correo electronico</label>
                                      <input type="email" class="form-control" id="inputEmail" placeholder="" value="" name="email"
                                          required>
                                  </div>
                              </div>
                              <div class="row" style="margin:1vmax 1vmax 1vmax 1vmax;">
                                  <div class="col-md-6">
                                      <label for="inputContrseña" class="form-label textoInputs">Contraseña</label>
                                      <div class="input-group mb-6">
                                          <input type="password" class="form-control " id="inputContrseña" name="password"
                                              pattern="(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@#$%^&*_=+-]).{8,32}$"
                                              placeholder="" value=""
                                              onfocus="mostrarValidaciones(document.getElementById('validaciones1').id);"
                                              onblur="ocultarValidaciones(document.getElementById('validaciones1').id);"
                                              oninput="setPasswordConfirmValidity();" onkeyup="checkPassword(this.value);"
                                              required>
                                          <span class="btn-action-icon" id="togglePassword1"
                                              onclick="togglePasswordF1();"></span>
                                          <div id="validaciones1" class="d-none">
                                              <span class="d-flex align-items-center" id="requisitos">&nbsp Debe contener
                                                  al
                                                  menos: </span>
                                              <li class="d-flex align-items-center" id="lower">Una letra minuscula. &nbsp
                                              </li>
                                              <li class="d-flex align-items-center" id="upper">Una letra mayuscula. &nbsp
                                              </li>
                                              <li class="d-flex align-items-center" id="number">Un numero. &nbsp </li>
                                              <li class="d-flex align-items-center" id="special">Un caracter especial.
                                                  &nbsp </li>
                                              <li class="d-flex align-items-center" id="length">8 caracteres. &nbsp </li>
                                          </div>
                                      </div>
                                  </div>
                                  <div class="col-md-6">
                                      <label for="inputConfirmarContrseña" class="form-label textoInputs">Confirmar
                                          contraseña</label>
                                      <div class="input-group mb-6">
                                          <input type="password" class="form-control " id="inputConfirmarContrseña"
                                              pattern="(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@#$%^&*_=+-]).{8,32}$"
                                              placeholder="" value=""
                                              onfocus="mostrarValidaciones(document.getElementById('validaciones2').id);"
                                              onblur="ocultarValidaciones(document.getElementById('validaciones2').id);"
                                              oninput="setPasswordConfirmValidity();" required>
                                          <span class="btn-action-icon" id="togglePassword2"
                                              onclick="togglePasswordF2();"></span>
                                          <div id="validaciones2" class="d-none">
                                              <li class="d-flex align-items-center" id="contrseñaNoCoincide">&nbsp Las
                                                  contraseñas
                                                  no
                                                  coinciden</li>
                                              <li class="d-flex align-items-center d-none" id="contrseñaCoincide">&nbsp
                                                  Las
                                                  contraseñas coinciden</li>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                              <div class="d-flex justify-content-center" style="margin: 10px 0px 20px 0px;">
                                  <button class=" btn-custom " type="submit" id="idRegistrarse">Registrar</button>
                              </div>
                          </form>
                      </main>


                  </div>

              </div>

          </div>
      </div>
      <!--Fin Modal Miembro -->


{{/partial}}
{{> baseOK.hbs }}